<launch>
	<include file="$(find robot_localization)/launch/bugcar_robot_localization.launch" />
	<include file="$(find twist_mux)/launch/simple_twist_mux.launch"/>
	<include file="$(find ublox_gps)/launch/single_gps.launch" />
	<include file="$(find roboclaw_node)/launch/roboclaw.launch">
	    <arg name="dev" value="/dev/ttyUSB_roboclaw"/>
	    <arg name="baud" value="115200"/>
	    <arg name="ticks_per_meter" value="1833.5"/>
	</include>
	
	<node pkg="rosserial_python" type="serial_node.py" name="rc_node" output="screen" args="-port /dev/ttyUSB_arduino -baud 115200"/>
	<node pkg="bno055_usb_stick" type="bno055_usb_stick_node" name="bno055_node">
	    <param name="port" value="/dev/ttyUSB_BNOimu" />
            <param name="publish_tf" value="false" />
	    <param name="topic" value="/imu/data"/>
	</node>	
</launch>
