<?xml version="1.0" encoding="UTF-8"?>

<launch>
  <arg name="node_name" default="gps" />
  <arg name="param_file_name" doc="name of param file, e.g. rover" default="single_gps_config" />
  <arg name="output" default="screen" />
  <arg name="respawn" default="true" />
  <arg name="respawn_delay" default="30" />
  <arg name="clear_params" default="false" />
  

  <group>
    <node pkg="ublox_gps" type="ublox_gps" name="$(arg node_name)"
      output="$(arg output)" 
      clear_params="$(arg clear_params)"
      respawn="$(arg respawn)" 
      respawn_delay="$(arg respawn_delay)">
    <param name="device" value="/dev/ttyUSB_gps1" />
    <param name="frame_id" value="base_link" />
    <param name="velocity_frame_id" value="map" />
      
    </node>
  </group>

</launch>

