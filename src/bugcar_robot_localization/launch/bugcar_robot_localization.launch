<launch> 
	<rosparam command="load" file="$(find robot_localization)/params/dual_ekf_navsat.yaml" />

	<!-- =========================================================== -->
	<node pkg="robot_localization" type="ekf_localization_node" name="ekf_se_odom" clear_params="true">

		<!--  Placeholder for input topic remapping  -->
		<remap from="odometry/wheel" to="odom"/>
		<remap from="imu/data" to="imu"/>
		<!--  Placeholder for output topic remapping  -->
		<remap from="odometry/filtered" to="odometry/filtered_odom_baselink"/>

	</node>
	<!-- =========================================================== -->

</launch>
